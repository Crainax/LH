                    call ModifyHeroStat( bj_HEROSTAT_AGI, udg_Unit, bj_MODIFYMETHOD_ADD, udg_TianTing_Zengyi[21] )
                    call ModifyHeroStat( bj_HEROSTAT_STR, udg_Unit, bj_MODIFYMETHOD_ADD, udg_TianTing_Zengyi[41] )
