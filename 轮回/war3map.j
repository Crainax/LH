            if ((YDWEGetIntegerByString("天庭", "数量") >= I3(udg_Nandu_JJJ < 5,10,udg_Nandu_JJJ + 3)) and (GetOwningPlayer(GetTriggerUnit()) == Player(PLAYER_NEUTRAL_PASSIVE))) then
