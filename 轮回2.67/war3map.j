		//‰ªôÁÇºÊàñÁîüÂÖã
	    		set ydl_group=CreateGroup()
				call GroupEnumUnitsInRange(ydl_group, x, y, 900, Condition(function MonsterSpell__TSpellQianFaEnemyFilter))
				loop
				    set ydl_unit=FirstOfGroup(ydl_group)
				    exitwhen ydl_unit == null
