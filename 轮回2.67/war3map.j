	    call CreateTextTagA(("Ëôö--ÂçÉÁΩö‰πãÂÖâ" ) , ( GetAttacker() ) , (( 100 )*1.0) , (( 100 )*1.0) , (( 0 )*1.0) , (( 2)*1.0) , 16) // INLINED!!

	    call PolledWait(5.00)
	    call EnableTrigger(GetTriggeringTrigger())

	    call DestroyGroup(ydl_group)
