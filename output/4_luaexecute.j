			call DisplayTextToPlayer(ConvertedPlayer(index), 0., 0., "|cFFFF66CC【消息】|r你没有"+I2S(lumber)+"的木头！")
			return
		endif
		call AdjustPlayerStateBJ(-1*lumber,ConvertedPlayer(index),PLAYER_STATE_RESOURCE_LUMBER)
