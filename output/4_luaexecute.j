	        call PauseTimer(t)
	        call FlushChildHashtable(spellTable,id)
	        call DestroyTimer(t)
			call SetUnitTimeScale(kaisa,3.00)
	    endif
	    call DestroyGroup(temp)
	    set temp = null
	    set t = null
	endfunction
