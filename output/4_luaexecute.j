		if ((GetUnitTypeId(u) == 'h01U' or GetUnitTypeId(u) == 'h02D') and GetTriggerUnit() == udg_H[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]) then
				call HG(GetTriggerUnit())
