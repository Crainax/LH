    /*
        获取彩名
    */
    function GetColorString takes string s returns string
		local integer length = StringLength(s)
		local string result = ""
		local integer i =1
		if (length == 0) then
			return ""
		endif
		loop
			exitwhen i > length
			set result = result + GetRandomColor() + SubString(s, (i)-1, i+2)
			set i = i + 3
		endloop
        return result + "|r"
    endfunction
