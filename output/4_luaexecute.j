	/*
	    主英雄使用技能
	*/
	private function TSpellLichiAct takes nothing returns nothing
		if (GetSpellAbilityId() == 'A0MJ') then
			call Yingyanjue(GetSpellAbilityId(),GetUnitX(lichi) + 250 * Cos((GetUnitFacing(lichi))*0.0174538),GetUnitY(lichi) + 250 * Sin((GetUnitFacing(lichi))*0.0174538))
		elseif (GetSpellAbilityId() == 'A0MK') then
			call Yingyanjue(GetSpellAbilityId(),GetUnitX(lichi) + 500 * Cos((GetUnitFacing(lichi))*0.0174538),GetUnitY(lichi) + 500 * Sin((GetUnitFacing(lichi))*0.0174538))
		elseif (GetSpellAbilityId() == 'A0ML') then
			call Yingyanjue(GetSpellAbilityId(),GetUnitX(lichi) + 750 * Cos((GetUnitFacing(lichi))*0.0174538),GetUnitY(lichi) + 750 * Sin((GetUnitFacing(lichi))*0.0174538))
		elseif (GetSpellAbilityId() == 'A0MM') then
			call Yingyanjue(GetSpellAbilityId(),GetUnitX(lichi) + 1000 * Cos((GetUnitFacing(lichi))*0.0174538),GetUnitY(lichi) + 1000 * Sin((GetUnitFacing(lichi))*0.0174538))
		elseif (GetSpellAbilityId() == 'A0MO') then
			//湮魂印
			call Yanhunyin(GetSpellTargetUnit())
		elseif (GetSpellAbilityId() == 'A0MP') then
			//神月盈
			call Shenyueying()
		elseif (GetSpellAbilityId() == 'A0MQ') then
			//囚天地
			call Qiutiandi()
		endif
