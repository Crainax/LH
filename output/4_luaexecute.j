	*/
	function Get14Word takes player p returns string
		local string s = ""
		local integer i = 1
		loop
			exitwhen i > 14
			set s = s + S3(IsHasWord(p,i),"|cffff00ff","|cffffffff") + GetZhongqiuWord(i) + "|r"
			if (i == 7) then
				set s = s + "|cffff00ff，|r"
			elseif(i == 14) then
