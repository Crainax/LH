	    //刷新伤害,还有每秒判断形态是否扣血,还有加属性的判断
	    call TimerStart(CreateTimer(),1,true,function FlashXinglongDamage)
	    //升级回血及造成伤害
	    set TSpellXinglongUpdate = CreateTrigger()
