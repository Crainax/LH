    local integer ydl_localvar_step = LoadInteger(YDLOC, GetHandleId(GetTriggeringTrigger()), 0xCFDE6C76) 
 set ydl_localvar_step = ydl_localvar_step + 3 
 call SaveInteger(YDLOC, GetHandleId(GetTriggeringTrigger()), 0xCFDE6C76, ydl_localvar_step) 
 call SaveInteger(YDLOC, GetHandleId(GetTriggeringTrigger()), 0xECE825E7, ydl_localvar_step)
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = 6
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ((GetPlayerController(ConvertedPlayer(bj_forLoopAIndex)) == MAP_CONTROL_USER) and (GetPlayerSlotState(ConvertedPlayer(bj_forLoopAIndex)) == PLAYER_SLOT_STATE_PLAYING)) then
            call SaveInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 584838821, ( GetHeroStr( udg_H[bj_forLoopAIndex], true) / 10 ))
            call SaveInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 1695380770, ( GetHeroAgi( udg_H[bj_forLoopAIndex], true) / 10 ))
            call SaveInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 1695380769, ( GetHeroInt( udg_H[bj_forLoopAIndex], true) / 10 ))
            if ((GetUnitState( udg_H[bj_forLoopAIndex], UNIT_STATE_MAX_LIFE) <= 1000000000.00)) then
                call SaveInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 1695380764, ( R2I(GetUnitState( udg_H[bj_forLoopAIndex], UNIT_STATE_MAX_LIFE)) / 100 ))
            else
            endif
            call SaveInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 1695380763, ( LoadInteger(YDHT, GetHandleId( udg_H[bj_forLoopAIndex]), 2098427212) / 10 ))
            call SaveInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 1695380766, ( LoadInteger(YDHT, GetHandleId( udg_H[bj_forLoopAIndex]), 18117978) / 2 ))
            set udg_Zhandouli[bj_forLoopAIndex] = ( LoadInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 584838821) + ( LoadInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 1695380770) + ( LoadInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 1695380769) + ( LoadInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 1695380764) + ( LoadInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 1695380763) + LoadInteger(YDLOC, GetHandleId(GetTriggeringTrigger())*ydl_localvar_step, 1695380766) ) ) ) ) )
            call LeaderboardSetPlayerItemValueBJ( Player(10), udg_Paihang[bj_forLoopAIndex], udg_Zhandouli[bj_forLoopAIndex] )
