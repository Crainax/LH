	function ReduceHeroStrF takes unit hero,integer i returns nothing
		local integer delta
		local integer index
		if (GetHeroStr(hero,true) - i >= 0) then
			call SetHeroStr(hero,GetHeroStr(hero,true)-i,true)
			call FlashHeroStr(GetOwningPlayer(hero))
