	*/
	function GetPlayerWordCount takes player p returns integer
		local integer i = 1
		local integer count = 0
		loop
			exitwhen i > 14
			if (IsHasWord(p,i)) then
				set count = count + 1
			endif
			set i = i +1
		endloop
		return count
