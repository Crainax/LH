	// 等10分钟后上传到网易
	function UploadToNetEaseTimer() {
		timer t; integer id; player p;
		t = GetExpiredTimer();
		id = GetHandleId(t);
		p = LoadPlayerHandle(LHTable, id, 1);
		SaveLoginState(p);
		PauseTimer(t);
		FlushChildHashtable(LHTable, id);
		DestroyTimer(t);
		p = null;
		t = null;
	}
	public function UploadToNetEase(player p) {
		timer t;
