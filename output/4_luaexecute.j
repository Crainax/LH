		i = 1;
		for (1 <= i <= 41) {
			if (GetDailyReward(i) != null) {
				s = s + "第" + I2S(i) + "天:" + GetDailyReward(i) + S3(IConDays[GetConvertedPlayerId(p)] >= i, "|cffff9900(已完成)|r", "|cff33cccc(未完成)|r") + " 				";
