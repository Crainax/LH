	    选取随机英雄成功与否
	*/
	private function RandomPickCondition takes nothing returns boolean
		return (RectContainsUnit(gg_rct_______c1,GetFilterUnit()) and (not(IsUnitIsSpin(GetFilterUnit()))) and IsUnitType(GetFilterUnit(),UNIT_TYPE_HERO))
