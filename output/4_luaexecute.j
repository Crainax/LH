	    call CreateSpellTextTag("臣服于我",GetAttacker(),100,0,0,2)
	    call DestroyEffect(AddSpecialEffect("Abilities\\Spells\\Undead\\RaiseSkeletonWarrior\\RaiseSkeleton.mdl", GetUnitX(GetTriggerUnit()), GetUnitY(GetTriggerUnit()) ))
	endfunction
