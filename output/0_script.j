                call YDLocal1Set(integer, "i4", ( R2I(GetUnitStateSwap(UNIT_STATE_MAX_LIFE, udg_H[GetForLoopIndexA()])) / 100 ))
            else
            endif
            call YDLocal1Set(integer, "i5", ( YDUserDataGet(unit, udg_H[GetForLoopIndexA()], "GJ", integer) / 10 ))
            call YDLocal1Set(integer, "i6", ( YDUserDataGet(unit, udg_H[GetForLoopIndexA()], "FY", integer) / 2 ))
            set udg_Zhandouli[GetForLoopIndexA()] = ( YDLocal1Get(integer, "i") + ( YDLocal1Get(integer, "i2") + ( YDLocal1Get(integer, "i3") + ( YDLocal1Get(integer, "i4") + ( YDLocal1Get(integer, "i5") + YDLocal1Get(integer, "i6") ) ) ) ) )
            call LeaderboardSetPlayerItemValueBJ( Player(10), udg_Paihang[bj_forLoopAIndex], udg_Zhandouli[bj_forLoopAIndex] )
        else
        endif
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
    call YDLocal1Release()
endfunction

