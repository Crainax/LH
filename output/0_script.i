	    set t = CreateTrigger()
	    call TriggerRegisterAnyUnitEventBJ( t, EVENT_PLAYER_UNIT_ATTACKED )
	    call TriggerAddCondition(t, Condition(function TDengAttackCon))
	    call TriggerAddAction(t, function TDengAttackAct)
		set t = null
	endfunction
