	private function JudgeLichiMove takes nothing returns nothing
		local integer i
		local integer index
    	if (NiLiX != GetUnitX(lichi) or NiLiY != GetUnitY(lichi)) then
    		set NiLiX = GetUnitX(lichi)
    		set NiLiY = GetUnitY(lichi)
