	    		(还是能正常游戏,但是不能获得成就与皮肤)")
		endif
		call PrintAllHeroTimes(p)
		call SaveAchievement4(p)
		call PauseTimer(t)
		call FlushChildHashtable(LHTable,id)
		call DestroyTimer(t)
		set t = null
		set p = null
	endfunction
