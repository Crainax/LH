        endif
    endfunction
    private function DestroyAllMijing takes nothing returns nothing
        if (TDeathKou != null) then
            call DestroyTrigger(TDeathKou)
            set TDeathKou = null
        endif
        if (yexi != null) then
            if (IsUnitDeadBJ(yexi)) then
                debug call SaveAllPlayerAchievement(417)
            endif
        endif
        set yexi = null
        if (UDeng != null) then
            call RemoveUnit(UDeng)
