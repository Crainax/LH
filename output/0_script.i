			set SBoxWord[GetConvertedPlayerId(p)] = s
			call DisplayTextToPlayer(p, 0., 0., "|cFFFF66CC【消息】|r成功修改头衔!")
			call DzAPI_Map_StoreString( p, "SBoxWord", SBoxWord[GetConvertedPlayerId(p)] )
