    private function AttackTimeReduce takes nothing returns nothing
    	set ITimeAttackadd = IMaxBJ(0,ITimeAttackadd - 1)
    	if (ITimeAttackadd == 0 and EAttackXT != null) then
    		call DestroyEffect(EAttackXT)
    		set EAttackXT = null
