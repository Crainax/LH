	/*
	    获取某个幻影对应的位置，下面的话是负数形式
	*/
	private function GetHuanyingIndex takes integer id returns integer
		if (ModuloInteger(id,2) == 1) then
			// 奇数
			return (id/2)+1
		else
			return -1 * (id/2)
		endif
	endfunction
