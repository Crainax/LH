	    	endif
	    	call SetUnitUserData(u,R2I(facing)+I3((i == 1 or i == (6 + 4 * GetComboMulti())),2000,0))
	    	call UnitApplyTimedLifeBJ( 12.00 + RJ2(xiaoting,5,0), 'BHwe',u )
