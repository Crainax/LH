			call DestroyEffect(AddSpecialEffect("Abilities\\Spells\\Human\\Resurrect\\ResurrectCaster.mdl", x,y ))
		endif
		call UnitApplyTimedLifeBJ( 3, 'BHwe',u )
        call IssuePointOrder(lichi,"move",GetUnitX(lichi),GetUnitY(lichi))
		set u = null
