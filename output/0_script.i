	*/
	private function ChangeFacing takes integer i,real facing returns nothing
		local unit temp = CreateUnit(GetOwningPlayer(xiaoting),'h024',GetUnitX(UArrow[i]),GetUnitY(UArrow[i]),facing)
		call RemoveUnit(UArrow[i])
		set UArrow[i] = temp
