        //秘境人数伤害的提高与伤害的减少
        call YDWESyStemAnyUnitDamagedRegistTrigger( t )
        call TriggerAddCondition(t, Condition(function DamageMijingCon))
        call TriggerAddAction(t, function DamageMijingAct)
        set t = null
	endfunction
